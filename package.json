  "bugs": {
    "url": "https://github.com/xteam-app/xteam/issues"
  },
  "homepage": "https://github.com/xteam-app/xteam#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "workspaces": {
    "packages": [
      "apps/expo-app",
      "packages/core",
      "packages/tsconfig"
    ]
  }
